{
    "UnityExe": "C:\\Program Files\\Unity\\Editor\\Unity.exe",

    "BuildDir": "/Path/To/Build/Folder",
    "ReleaseDir": "/Path/To/Zipped/Releases/Folder",

    "Targets": [
        "Mac64",
        "Win32"
    ],

    "AssemblyInfo": "Assets/Scripts/AssemblyInfo.cs",

    "DefinesAlways": "ANY_GLOBAL_DEFINES;THAT_SHOULD;BE_SET_FOR;ALL_BUILDS",
    "DefinesDevMode": "EXTRA_DEFINES_DEV_MODE",
    "DefinesNonDevMode": "EXTRA_DEFINES_PROD_MODE",
    "DefinesSteam": "EXTRA_DEFINES;STEAM_BUILD",
    "DefinesNonSteam": "EXTRA_DEFINES_NON_STEAM_BUILD",

    "BuildForSteam": true,
    "BuildForNonSteam": true,

    "BuildSteamDevMode": false,
    "BuildNonSteamDevMode": true,


    "ItchAppId": "user/app-name",
    "ItchChannelByTarget": {
        "Mac64": "osx-universal",
        "Win32": "windows"
    },


    "SteamAppId": "YourSteamAppId",
    "SteamDepotsByTarget": {
        "Mac64": "YourMacDepotId",
        "Win32": "YourWindowsDepotId"
    },
    "SteamLogin": "YourSteamReleaseUser"
}


